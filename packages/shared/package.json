{
  "name": "@custompackages/shared",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/client/index.d.ts",
        "default": "./dist/es/client/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/client/index.d.ts",
        "default": "./dist/cjs/client/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/es/server/index.server.d.ts",
        "default": "./dist/es/server/index.server.mjs"
      },
      "require": {
        "types": "./dist/cjs/server/index.server.d.ts",
        "default": "./dist/cjs/server/index.server.cjs"
      }
    },
    "./index.css": "./dist/es/client/index.css"
  },
  "source": "./src/index.ts",
  "main": "./dist/cjs/client/index.cjs",
  "module": "./dist/es/client/index.mjs",
  "types": "./dist/es/client/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "rollup -c --watch ./rollup.config.mjs",
    "build": "rollup -c ./rollup.config.mjs",
    "postbuild": "tsc --emitDeclarationOnly --declarationMap false --declaration --declarationDir dist/types",
    "lint": "eslint \"**/*.ts*\"",
    "test": "jest --passWithNoTests",
    "generate:component": "turbo gen react-component",
    "design-token": "node ./src/designTokens/generator.mjs"
  },
  "devDependencies": {
    "@svgr/plugin-jsx": "^8.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@types/babel__plugin-transform-runtime": "^7.9.5",
    "@types/node": "^20.12.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "dotenv": "^16.4.5",
    "esbuild-plugin-svgr": "^2.1.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "jest-config": "workspace:*",
    "node-fetch": "^3.3.2",
    "postcss-import": "^16.1.0",
    "rollup-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tslib": "^2.6.2",
    "tsup": "^5.12.9",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/runtime": "^7.24.4",
    "react": "^18.0.2"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.3.0"
  }
}
