{
  "name": "shared",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./index.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "gen:build": "tsup ./index.tsx --format esm,cjs --dts --external react",
    "gen:dev": "tsup ./index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"**/*.ts*\"",
    "test": "jest --passWithNoTests",
    "generate:component": "turbo gen react-component",
    "design-token": "node ./src/designTokens/generator.mjs",
    "icon": "node ./src/icons/generator.mjs",
    "convert": "node ./src/icons/profiles/generator.mjs"
  },
  "devDependencies": {
    "@svgr/plugin-jsx": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.6",
    "@types/testing-library__jest-dom": "^5.14.8",
    "dotenv": "^16.3.1",
    "esbuild-plugin-svgr": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "jest-config": "workspace:*",
    "node-fetch": "^3.3.2",
    "tsconfig": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^18.0.2"
  },
  "dependencies": {
    "crypto": "^1.0.1"
  }
}
