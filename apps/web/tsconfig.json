{
  "extends": "../../config/tsconfig/nextjs.json",
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es2017",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "auth.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
